#include <LiquidCrystal_I2C.h>


int Force_value=0;


LiquidCrystal_I2C lcd (0x27,16,2);
void setup()
{
  pinMode(A0,INPUT);
  lcd.init(); 
  lcd.backlight();     
}

void loop()
{
  lcd.print("       Loading force...     ");
  delay(250);
  lcd.clear();
  Force_value=analogRead(A0);
  
  lcd.print("Force_value");
}
